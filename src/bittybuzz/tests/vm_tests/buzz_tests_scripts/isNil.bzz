function functionType() {
}

function init() {
}


function step() {
    var nilVal = nil;
    var intVal = 42;
    var floatVal = 42.24;
    var stringVal = "Hello";
    var tableVal = {};
    var nativeClosureVal = functionType;
    var lambdaClosureVal = function(){};

    assertTrue(isNil(nilVal));
    assertFalse(isNil(intVal));
    assertFalse(isNil(floatVal));
    assertFalse(isNil(stringVal));
    assertFalse(isNil(tableVal));
    assertFalse(isNil(nativeClosureVal));
    assertFalse(isNil(lambdaClosureVal));
}
