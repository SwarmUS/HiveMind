function function_type() {
}

function init() {
}

function step() {
    var nil_val = nil;
    var int_val = 42;
    var float_val = 42.24;
    var string_val = "Hello";
    var table_val = {.0=42, .1=43};
    var native_closure_val = function_type;
    var lambda_closure_val = function(){};

    assert_false(is_lambda_closure(nil_val));
    assert_false(is_lambda_closure(int_val));
    assert_false(is_lambda_closure(float_val));
    assert_false(is_lambda_closure(string_val));
    assert_false(is_lambda_closure(table_val));
    assert_false(is_lambda_closure(native_closure_val));
    assert_true(is_lambda_closure(lambda_closure_val));
}
