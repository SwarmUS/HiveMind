function function_type() {
}

function init() {
}


function step() {
    var nil_val = nil;
    var int_val = 42;
    var float_val = 42.24;
    var string_val = "Hello";
    var table_val = {};
    var native_closure_val = function_type;
    var lambda_closure_val = function(){};

    assert_true(isnil(nil_val));
    assert_false(isnil(int_val));
    assert_false(isnil(float_val));
    assert_false(isnil(string_val));
    assert_false(isnil(table_val));
    assert_false(isnil(native_closure_val));
    assert_false(isnil(lambda_closure_val));
}
