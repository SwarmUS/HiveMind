function function_type() {
}

function init() {
}


function step() {
    var nil_val = nil;
    var int_val = 42;
    var float_val = 42.24;
    var string_val = "Hello";
    var table_val = {};
    var native_closure_val = function_type;
    var lambda_closure_val = function(){};

    assert_true(is_nil(nil_val));
    assert_false(is_nil(int_val));
    assert_false(is_nil(float_val));
    assert_false(is_nil(string_val));
    assert_false(is_nil(table_val));
    assert_false(is_nil(native_closure_val));
    assert_false(is_nil(lambda_closure_val));
}
